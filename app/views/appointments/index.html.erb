<header>
  <h2>Appointments</h2>

  <ul class="actions">
    <li>
      <%= link_to "New Appointment", new_appointment_path, class: "new" %>
    </li>
  </ul>
</header>

<div id="appointments">
  <% @appointments.each do |appointment| %>
    <h2><%= link_to appointment.date, appointment %></h2>
  <% end %>
</div>
